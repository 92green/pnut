{
  "name": "pnut",
  "version": "0.7.2",
  "description": "The new and improved pnut",
  "files": [
    "lib"
  ],
  "release": {
    "name": "HurtfulIguana"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bigdatr/pnut.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib --ignore 'src/**/__test__/*.js'",
    "watch": "rm -rf lib && babel src --out-dir lib --ignore 'src/**/__test__/*.js' -w",
    "flow": "blueflag-test flow",
    "flow-coverage": "blueflag-test flow-coverage -M",
    "lint": "blueflag-test lint",
    "prepublishOnly": "yarn build",
    "test": "yarn jest",
    "test-all": "yarn lint && yarn flow && yarn test"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "babel-plugin-extract-flow-types": "^1.0.1",
    "babel-plugin-flow-react-proptypes": "^25.0.0",
    "babel-preset-blueflag": "^1.0.0",
    "babel-runtime": "^6.5.0",
    "blueflag-test": "^0.22.0",
    "flow-bin": "^0.96.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.2",
    "d3-array": "^1.2.4",
    "d3-interpolate": "^1.3.2",
    "d3-scale": "^2.2.2",
    "d3-shape": "^1.3.5",
    "prop-types": "^15.7.2",
    "stampy": "^0.44.0"
  },
  "main": "lib/index.js"
}
