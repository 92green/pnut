{
  "name": "pnut",
  "version": "0.0.0",
  "description": "The new and improved pnut",
  "repository": {
    "type": "git",
    "url": "git@github.com:bigdatr/pnut.git"
  },
  "license": "MIT",
  "scripts": {
    "ava": "NODE_ENV=test ava",
    "build": "rm -rf lib && yarn run css && babel src --out-dir lib ",
    "build-examples": "cd example && yarn install && yarn run build && cd -",
    "check-coverage": "NODE_ENV=test nyc check-coverage --branches 100.0  --functions 100.0 --lines 100.0",
    "css": "yarn run css-base-combined && yarn run css-default-combined && yarn run css-base && yarn run css-default && yarn run postcss",
    "css-base": "sh scripts/css-base.sh",
    "css-base-combined": "sh scripts/css-base-combined.sh",
    "css-default": "sh scripts/css-default.sh",
    "css-default-combined": "sh scripts/css-default-combined.sh",
    "deploy-to-gh-pages": "yarn run build && yarn run build-examples && yarn run docs && node scripts/publish.js",
    "dev": "babel src -d lib --watch",
    "docs": "jsdoc -c jsdoc.json",
    "postcss": "postcss --use autoprefixer -r lib/**/*.css",
    "prepublish": "node node_modules/node-sass/scripts/install.js && yarn run build",
    "flow": "blueflag-test flow",
    "lint": "blueflag-test lint",
    "test": "blueflag-test test",
    "coverage": "blueflag-test coverage --min-coverage=0",
    "test-all": "yarn lint && yarn flow && yarn coverage"
  },
  "devDependencies": {
    "app-module-path": "^2.0.0",
    "autoprefixer": "^6.5.2",
    "babel-cli": "^6.5.0",
    "babel-core": "^6.5.0",
    "babel-preset-blueflag": "^0.6.0",
    "blueflag-test": "^0.12.0",
    "cheerio": "^0.22.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gh-pages": "^0.11.0",
    "git-url-parse": "^6.0.1",
    "jsdoc": "^3.4.2",
    "jsdoc-babel": "^0.2.1",
    "jsdoc-react-proptypes": "^1.0.0",
    "jsdom": "^9.8.3",
    "jsdonk": "0.4.10",
    "node-sass": "^3.11.2",
    "postcss-cli": "^2.6.0",
    "proto-babel-plugin-extract-flow-types": "git+ssh://git@github.com/blueflag/proto-babel-plugin-extract-flow-types.git#v0.1.1",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^16.2.0"
  },
  "peerDependencies": {
    "react": "^15.3.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "d3-interpolate": "^1.1.3",
    "d3-scale": "^1.0.4",
    "d3-shape": "^1.0.4",
    "immutable": "^3.8.1",
    "immutable-math": "^0.1.3",
    "prop-types": "^15.6.1",
    "stampy": "^0.37.4",
    "unmutable": "^0.23.0"
  },
  "main": "lib/index.js"
}
